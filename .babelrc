{
  "presets": [
    "es2015",
    "react"
  ],
  "plugins": [
    "syntax-decorators",
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-export-extensions",
    ["import", {
      "libraryName": "antd",
      "libraryDirectory": "lib",   // default: lib
      "style": true
    }]
  ]
}